;  Description      : Linker Control File (scatter-loading) for IAP.
;-----------------------------------------------------------------------------------------------------------*/

IAP 0x00000000 0x00001000         ; 4 kB for IAP
{
  IAP +0
  {
    *.o (IAP, +FIRST)     ; IAP Program
  }
}

AP 0x00001000                     ; User application start from 0x1000
{
  AP +0
  {
    *.o (RESET, +FIRST)
    * (+RO)
  }
  RAM 0x20000010
  {
    * (+RW +ZI)
  }
}
